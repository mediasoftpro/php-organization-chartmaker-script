{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown", "plugins/verboseOutput"],
    "markdown": {
        "parser": "gfm"
    },
    "explain":true,
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "css":{
        "demo":"../demo/demo-all.css",
        "gollum":"../doc/gollum-template.css"
    },
    "jsVersion": 180,
    "layout":"jsplumb-apidoc.tmpl"
}